@{
    ViewBag.Title = "Parker Gunnell's First ASP.NET Website";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/css/main.css" rel="stylesheet" />
    <!--An icon for the website (displayed on the tab next to the title)-->
    <link rel="icon"
          type="image/png"
          href="~/img/ParkerIcon.png">
    

</head>
<body>
    <header>
        <!--Header that lists your preferred name-->
        <h1>Parker Gunnell</h1>
        <menu>
            <ul>
                <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                <li><a asp-controller="Home" asp-action="Page">Page</a></li>
            </ul>
        </menu>
    </header>

    @RenderBody()

    <script src="~/node_modules/jquery/dist/jquery.min.js"></script>
    <script src="~/js/main.js"></script>
    <footer>
        <p class="footer">Parker Gunnell -- Section 2 Group 11</p>
    </footer>
</body>

</html>